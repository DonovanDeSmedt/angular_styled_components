{
  "name": "angular-styled-components",
  "version": "0.0.5",
  "description": "Styled component package for Angular",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "ISC",
  "keywords": ["angular", "styled-components", "css", "css-in-js"],
  "author": {
    "name": "Donovan De Smedt"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:umd && npm run copy",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch":
      "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "copy":
      "node tools/copy-package.js && node tools/cleanup.js && node tools/copy-readme.js",
    "ci": "npm run build",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "deploy": "node scripts/publish.js",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "bump-version": "node scripts/prepush.js",
    "prepush": "npm run bump-version",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci"
  },
  "dependencies": {
    "rxjs": "^5.5.2"
  },
  "peerDependencies": {
    "@angular/common": "5.0.1",
    "@angular/core": "5.0.1"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.2",
    "@angular/cdk": "5.0.1",
    "@angular/forms": "5.0.1",
    "@angular/animations": "5.0.1",
    "@angular/common": "5.0.1",
    "@angular/compiler": "5.0.1",
    "@angular/compiler-cli": "5.0.1",
    "@angular/core": "5.0.1",
    "@angular/http": "5.0.1",
    "@angular/platform-browser": "5.0.1",
    "@angular/platform-browser-dynamic": "5.0.1",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@types/jasmine": "^2.5.47",
    "@types/karma": "^1.7.0",
    "@types/node": "^8.0.0",
    "@types/webpack": "^3.0.13",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^2.2.0",
    "codelyzer": "^3.0.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "child-process-promise": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.16.2",
    "inquirer": "^4.0.1",
    "node-sass-tilde-importer": "^1.0.1",
    "path": "^0.12.7",
    "run-sequence": "^2.2.1",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "husky": "^0.14.3",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.6.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash-webpack-plugin": "^0.11.4",
    "node-sass": "^4.9.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "source-map-explorer": "^1.3.3",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "2.4.2",
    "webpack": "^3.8.1",
    "webpack-angular-externals": "^1.0.2",
    "webpack-rxjs-externals": "^1.0.0",
    "zone.js": "^0.8.18"
  }
}
